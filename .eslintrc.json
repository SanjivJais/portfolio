{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_$", // Ignore only `_` in function arguments
        "varsIgnorePattern": "^_$" // (Optional) Ignore only `_` in variables, if needed
      }
    ]
  }
}